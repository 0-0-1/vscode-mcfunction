{
    "autoClosingPairs": [
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "comments": {
        "lineComment": "#"
    },
    "folding": {
        "offSide": false,
        "markers": {
            "start": "^#[Ss]ection",
            "end": "^[eE]nd[Ss]ection"
        }
    },
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "\"",
            "\""
        ]
    ]
}